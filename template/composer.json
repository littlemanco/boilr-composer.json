{
    "name": "{{Vendor}}/{{ Name }}",
    "description": "{{ Description }}",
    "type": "{{ Type }}",
    "homepage": "{{ GithubRepo }}",
    "license": "{{ License }}",
    "authors": [
        {
            "name": "{{ AuthorName }}",
            "email": "{{ AuthorEmail }}",
            "homepage": "{{ AuthorHomepage }}",
            "role": "{{ AuthorRole }}"
        }
    ],
    "support": {
        "email": "{{ AuthorEmail }}",
        "issues": "{{ GithubRepo }}/issues",
        "wiki": "{{ GithubRepo }}/wiki",
        "source": "{{ GithubRepo }}"
    }, 
    "require": {
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "{{ Vendor }}\\": "src/"
        }
    },
    "bin": [
    ],
    "archive": {
        "exclude": [
            "test",
            "hack",
            "hooks",
            ".arclint"
        ]
    },
    "config": {
        "bin-dir":             "bin",
        "store-auths":         false,
        "optimize-autoloader": true,
        "sort-packages":       true,
        "apcu-autoloader":     true,
        "notify-on-install":   false
    },
    "minimum-stability": "{{ MinimumStability }}"
}
